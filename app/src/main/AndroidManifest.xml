<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.splashactivity">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/guitar"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/guitar"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.PlayActivity"></activity>
        <activity android:name=".activity.LoginActivity" />
        <activity
            android:name=".activity.Home_Activity"
            android:label="@string/title_activity_home_"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.Splash_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>